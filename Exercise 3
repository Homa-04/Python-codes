# ایجاد یک لیست برای ذخیره مشخصات دانشجوها
students = []

# حلقه برای گرفتن مشخصات ۲۰ دانشجو
for i in range(20):
    print(f"Enter details for student {i+1}:")
    name = input("Enter the name: ")
    last_name = input("Enter the last name: ")
    age = int(input("Enter the age: "))
    city = input("Enter the city: ")

    # ایجاد یک دیکشنری برای ذخیره مشخصات هر دانشجو
    student_info = {
        "name": name,
        "last_name": last_name,
        "age": age,
        "city": city
    }
    
    # اضافه کردن دیکشنری مشخصات دانشجو به لیست
    students.append(student_info)

# چاپ مشخصات همه دانشجوها
print("All student details:")
for student in students:
    print(student)
